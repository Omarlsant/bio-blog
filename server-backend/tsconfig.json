{
  "compilerOptions": {
    "moduleResolution": "node",
    "noImplicitAny": true,
    "target": "ES6", 
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "noEmit": true
  }
}